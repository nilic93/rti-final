{
  "name": "angular-serverless",
  "version": "0.0.0",
  "scripts": {
    "ng": "node_modules/.bin/ng",
    "start": "node_modules/.bin/ng serve",
    "build": "node_modules/.bin/ng build",
    "lint": "node_modules/.bin/ng lint",
    "e2e": "node_modules/.bin/ng e2e",
    "dev:ssr": "node_modules/.bin/ng run angular-serverless:serve-ssr",
    "serve:ssr": "node dist/angular-serverless/server/main.js",
    "build:ssr": "node_modules/.bin/ng build --prod && ng run angular-serverless:server:production",
    "prerender": "node_modules/.bin/ng run angular-serverless:prerender",
    "serve:serverless": "node_modules/.bin/serverless offline start",
    "build:serverless": "node_modules/.bin/ng build --prod && ng run angular-serverless:serverless:production",
    "deploy": "node_modules/.bin/serverless deploy",
    "serverless:credentials": "node_modules/.bin/serverless config credentials --profile default --provider aws --key seiji --secret villafranca --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.6",
    "@angular/common": "13.2.6",
    "@angular/compiler": "13.2.6",
    "@angular/core": "13.2.6",
    "@angular/forms": "13.2.6",
    "@angular/platform-browser": "13.2.6",
    "@angular/platform-browser-dynamic": "13.2.6",
    "@angular/platform-server": "13.2.6",
    "@angular/router": "13.2.6",
    "@ng-toolkit/serverless": "^8.1.0",
    "@nguniversal/express-engine": "13.0.2",
    "aws-serverless-express": "3.4.0",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.5",
    "@nguniversal/builders":"13.0.2",
    "@angular/cli": "13.2.5",
    "@angular/compiler-cli": "13.2.6",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "serverless": "^2.18.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-offline": "^6.8.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.4.4",
    "serverless-domain-manager": "5.2.0",
    "serverless-prune-plugin": "^1.6.1"
  }
}
